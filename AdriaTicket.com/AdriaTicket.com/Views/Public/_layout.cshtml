<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>

    <!-- fonts -->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,800,700&subset=latin,latin-ext' rel='stylesheet' type='text/css'>

    <!-- css -->
    <link rel="stylesheet" type="text/css" href="~/Library/sliderengine/amazingslider-1.css">
    <link rel="stylesheet" href="~/Library/CSS/bootstrap/bootstrap.css" type="text/css">
    <link rel="stylesheet" href="~/Library/CSS/bootstrap/bootstrap-theme.css" type="text/css">
    <link rel="stylesheet" href="~/Library/CSS/bootstrap/font-awesome.min.css" type="text/css">
    <link rel="stylesheet" href="~/Library/CSS/bk-main.css" type="text/css">

</head>
<body ng-app="adriaticket">
    <!-- Facebook plugin -->
    <div id="fb-root"></div>
    <script>
(function (d, s, id) {
    var js, fjs = d.getElementsByTagName(s)[0];
    if (d.getElementById(id)) return;
    js = d.createElement(s); js.id = id;
    js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.3";
    fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>
    <!-- / Facebook plugin -->
    <div id ="page-container">
        <div id="header">
            @Html.Partial("Public/Header")
        </div>
        <div id="main-content" class="container">
            @RenderBody()
        </div>
        <div id="footer">
            @Html.Partial("Public/Footer")
        </div>
    </div>
</body>
    <!-- scripts -->
<script src='~/Library/Angular/angular.min.js' type="text/javascript"></script>
<script src="~/Library/Angular/angular-route.js" type="text/javascript"></script>
<script src='~/Library/Angular/app.js' type="text/javascript"></script>
<script src="~/Library/Angular/Controllers/AdriaTicketController.js" type="text/javascript"></script>
<script src='~/Library/JS/bk-main.js' type="text/javascript"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
<script src='~/Library/JS/bootstrap/bootstrap.min.js' type="text/javascript"></script>

<script src="~/Library/sliderengine/jquery.js"></script>
<script src="~/Library/sliderengine/amazingslider.js"></script>
<script src="~/Library/sliderengine/initslider-1.js"></script>
</html>